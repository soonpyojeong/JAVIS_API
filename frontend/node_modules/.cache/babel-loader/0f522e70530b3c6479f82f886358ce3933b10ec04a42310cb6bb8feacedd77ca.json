{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport axios from \"axios\";\nimport Chart from \"chart.js/auto\"; // Chart.js ÏûêÎèô Î°úÎìú\nimport ChartDataLabels from 'chartjs-plugin-datalabels'; // ÌîåÎü¨Í∑∏Ïù∏ import\n\nexport default {\n  data() {\n    return {\n      selectedDb: \"DBÏ°∞ÌöåÏ§ë\",\n      tbList: [],\n      tablespaces: [],\n      searchQuery: \"\"\n    };\n  },\n  computed: {\n    filteredTablespaces() {\n      return this.tablespaces.filter(ts => ts.id.tsName.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }\n  },\n  methods: {\n    formatNumber(number) {\n      return number.toLocaleString(); // Ï≤ú Îã®ÏúÑ Íµ¨Î∂Ñ Í∏∞Ìò∏ Ï∂îÍ∞Ä\n    },\n    fetchDbList() {\n      axios.get(\"/api/tb/list\").then(res => {\n        this.tbList = res.data;\n        if (this.tbList.length > 0) {\n          this.selectedDb = this.tbList[0];\n          this.fetchTablespaces(this.selectedDb);\n        }\n      }).catch(error => {\n        console.error(\"Error fetching DB list:\", error);\n      });\n    },\n    fetchTablespaces(dbName) {\n      axios.get(`/api/tb/${dbName}/tablespaces`).then(res => {\n        // Î≥ÄÍ≤ΩÏù¥ ÏóÜÏúºÎ©¥ ÏóÖÎç∞Ïù¥Ìä∏ÌïòÏßÄ ÏïäÏùå\n        if (JSON.stringify(this.tablespaces) === JSON.stringify(res.data)) return;\n        this.tablespaces = res.data || [];\n        this.$nextTick(() => {\n          this.tablespaces.forEach(ts => {\n            this.drawBarChart(ts);\n          });\n        });\n      }).catch(error => {\n        console.error(\"ÌÖåÏù¥Î∏îÏä§ÌéòÏù¥Ïä§ Îç∞Ïù¥ÌÑ∞ Í∞ÄÏ†∏Ïò§Í∏∞ Ïã§Ìå®:\", error);\n      });\n    },\n    drawBarChart(ts) {\n      if (!ts || !ts.id || !ts.id.tsName) {\n        console.error(\"Invalid ts object:\", ts);\n        return;\n      }\n      const canvasId = `chart-${ts.id.tsName.replace(/\\s+/g, '_')}`;\n\n      // VueÍ∞Ä DOM ÏóÖÎç∞Ïù¥Ìä∏ ÌõÑ Ïã§Ìñâ\n      this.$nextTick(() => {\n        const canvas = document.getElementById(canvasId);\n\n        // üö® canvasÍ∞Ä Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏúºÎ©¥ ÏóêÎü¨ Î∞©ÏßÄ\n        if (!canvas) {\n          console.error(`Canvas element not found: ${canvasId}`);\n          return;\n        }\n        const ctx = canvas.getContext('2d');\n\n        // Í∏∞Ï°¥ Ï∞®Ìä∏Í∞Ä ÏûàÏúºÎ©¥ ÏÇ≠Ï†ú\n        if (this.chartInstances && this.chartInstances[ts.id.tsName]) {\n          this.chartInstances[ts.id.tsName].destroy();\n        }\n\n        // chartInstances Í∞ùÏ≤¥ Ï¥àÍ∏∞Ìôî\n        if (!this.chartInstances) {\n          this.chartInstances = {};\n        }\n\n        // üî¢ ÏÇ¨Ïö©Î•† Î∞òÏò¨Î¶º (ÏÜåÏàòÏ†ê 1ÏûêÎ¶¨)\n        const roundedUsedRate = parseFloat(ts.usedRate.toFixed(1));\n\n        // ÏÉà Ï∞®Ìä∏ ÏÉùÏÑ±\n        this.chartInstances[ts.id.tsName] = new Chart(ctx, {\n          type: 'bar',\n          data: {\n            labels: ['ÏÇ¨Ïö©Î•†'],\n            datasets: [{\n              label: 'ÏÇ¨Ïö©Î•†',\n              data: [roundedUsedRate],\n              // Î∞òÏò¨Î¶ºÎêú Í∞í Ï†ÅÏö©\n              backgroundColor: 'rgba(75, 192, 192, 0.2)',\n              borderColor: 'rgb(75, 192, 192)',\n              borderRadius: 1,\n              borderWidth: 1\n            }]\n          },\n          options: {\n            responsive: true,\n            indexAxis: 'y',\n            scales: {\n              x: {\n                min: 0,\n                max: 100,\n                ticks: {\n                  display: false\n                },\n                // XÏ∂ï ÎààÍ∏à Ïà®ÍπÄ\n                grid: {\n                  display: false\n                },\n                // XÏ∂ï Í∑∏Î¶¨Îìú ÏÑ† Ïà®ÍπÄ\n                border: {\n                  display: false\n                } // XÏ∂ï Í≤ΩÍ≥ÑÏÑ† Ïà®ÍπÄ\n              },\n              y: {\n                beginAtZero: true,\n                display: false,\n                // YÏ∂ï Ïà®ÍπÄ\n                grid: {\n                  display: true\n                } // YÏ∂ï Í∑∏Î¶¨Îìú ÏÑ† Ïà®ÍπÄ\n              }\n            },\n            plugins: {\n              legend: {\n                display: false\n              },\n              // Î≤îÎ°Ä Ïà®ÍπÄ\n              datalabels: {\n                display: true,\n                align: ctx => ctx.dataset.data[0] >= 56 ? 'center' : 'end',\n                anchor: ctx => ctx.dataset.data[0] >= 56 ? 'center' : 'end',\n                formatter: value => `${value.toFixed(1)}%`,\n                // üî¢ ÏÜåÏàòÏ†ê 1ÏûêÎ¶¨ Ï†ÅÏö©\n                color: ctx => ctx.dataset.data[0] >= 85 ? 'red' : 'rgb(75, 192, 192)',\n                font: {\n                  weight: 'bold',\n                  size: 14\n                }\n              }\n            }\n          },\n          plugins: [ChartDataLabels]\n        });\n      });\n    }\n  },\n  mounted() {\n    this.fetchDbList();\n  }\n};","map":{"version":3,"names":["axios","Chart","ChartDataLabels","data","selectedDb","tbList","tablespaces","searchQuery","computed","filteredTablespaces","filter","ts","id","tsName","toLowerCase","includes","methods","formatNumber","number","toLocaleString","fetchDbList","get","then","res","length","fetchTablespaces","catch","error","console","dbName","JSON","stringify","$nextTick","forEach","drawBarChart","canvasId","replace","canvas","document","getElementById","ctx","getContext","chartInstances","destroy","roundedUsedRate","parseFloat","usedRate","toFixed","type","labels","datasets","label","backgroundColor","borderColor","borderRadius","borderWidth","options","responsive","indexAxis","scales","x","min","max","ticks","display","grid","border","y","beginAtZero","plugins","legend","datalabels","align","dataset","anchor","formatter","value","color","font","weight","size","mounted"],"sources":["D:\\JAVIS\\dongkukDBmon\\dongkukDBmon\\frontend\\src\\components\\TablespacesList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>{{ selectedDb }} DB - ÌÖåÏù¥Î∏îÏä§ÌéòÏù¥Ïä§ Î¶¨Ïä§Ìä∏</h2>\r\n\r\n    <!-- DB Î™©Î°ù -->\r\n    <div class=\"select-container\">\r\n      <select v-model=\"selectedDb\" @change=\"fetchTablespaces(selectedDb)\">\r\n        <option disabled value=\"DBÏ°∞ÌöåÏ§ë\">DBÏ°∞ÌöåÏ§ë</option>\r\n        <option v-for=\"(db, index) in tbList\" :key=\"index\" :value=\"db\">\r\n          {{ db }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- ÌÖåÏù¥Î∏î -->\r\n    <table class=\"tablespace-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Tablespace Ïù¥Î¶Ñ</th>\r\n          <th>TOTAL(MB)</th>\r\n          <th>USED(MB)</th>\r\n          <th>ÏÇ¨Ïö©Î•† (%)</th>\r\n          <th>FREE(MB)</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr\r\n          v-for=\"ts in filteredTablespaces\"\r\n          :key=\"ts.id.tsName\"\r\n          @dblclick=\"fetchRecentData(selectedDb, ts.id.tsName)\"\r\n        >\r\n          <td class=\"ts-name\">{{ ts.id.tsName }}</td>\r\n          <td class=\"used-size\">{{ formatNumber(ts.totalSize) }}</td>\r\n          <td class=\"used-size\">{{ formatNumber(ts.usedSize) }}</td>\r\n          <td class=\"used-rate\">\r\n            <!-- ÏÇ¨Ïö©Î•† Ï∞®Ìä∏ -->\r\n            <div class=\"used-rate-container\">\r\n              <canvas :id=\"'chart-' + ts.id.tsName\" class=\"rate-chart\" width=\"200\" height=\"100\"></canvas>\r\n            </div>\r\n          </td>\r\n          <td class=\"free-size\">{{ formatNumber(ts.freeSize) }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <p v-if=\"filteredTablespaces.length === 0\">Í≤ÄÏÉâ Í≤∞Í≥ºÍ∞Ä ÏóÜÏäµÎãàÎã§.</p>\r\n\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport Chart from \"chart.js/auto\"; // Chart.js ÏûêÎèô Î°úÎìú\r\nimport ChartDataLabels from 'chartjs-plugin-datalabels'; // ÌîåÎü¨Í∑∏Ïù∏ import\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      selectedDb: \"DBÏ°∞ÌöåÏ§ë\",\r\n      tbList: [],\r\n      tablespaces: [],\r\n      searchQuery: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    filteredTablespaces() {\r\n      return this.tablespaces.filter((ts) =>\r\n        ts.id.tsName.toLowerCase().includes(this.searchQuery.toLowerCase())\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    formatNumber(number) {\r\n      return number.toLocaleString(); // Ï≤ú Îã®ÏúÑ Íµ¨Î∂Ñ Í∏∞Ìò∏ Ï∂îÍ∞Ä\r\n    },\r\n    fetchDbList() {\r\n      axios\r\n        .get(\"/api/tb/list\")\r\n        .then((res) => {\r\n          this.tbList = res.data;\r\n          if (this.tbList.length > 0) {\r\n            this.selectedDb = this.tbList[0];\r\n            this.fetchTablespaces(this.selectedDb);\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching DB list:\", error);\r\n        });\r\n    },\r\n    fetchTablespaces(dbName) {\r\n      axios\r\n        .get(`/api/tb/${dbName}/tablespaces`)\r\n        .then((res) => {\r\n          // Î≥ÄÍ≤ΩÏù¥ ÏóÜÏúºÎ©¥ ÏóÖÎç∞Ïù¥Ìä∏ÌïòÏßÄ ÏïäÏùå\r\n          if (JSON.stringify(this.tablespaces) === JSON.stringify(res.data)) return;\r\n\r\n          this.tablespaces = res.data || [];\r\n          this.$nextTick(() => {\r\n            this.tablespaces.forEach((ts) => {\r\n              this.drawBarChart(ts);\r\n            });\r\n          });\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"ÌÖåÏù¥Î∏îÏä§ÌéòÏù¥Ïä§ Îç∞Ïù¥ÌÑ∞ Í∞ÄÏ†∏Ïò§Í∏∞ Ïã§Ìå®:\", error);\r\n        });\r\n    },\r\n\r\n    drawBarChart(ts) {\r\n        if (!ts || !ts.id || !ts.id.tsName) {\r\n            console.error(\"Invalid ts object:\", ts);\r\n            return;\r\n        }\r\n\r\n        const canvasId = `chart-${ts.id.tsName.replace(/\\s+/g, '_')}`;\r\n\r\n        // VueÍ∞Ä DOM ÏóÖÎç∞Ïù¥Ìä∏ ÌõÑ Ïã§Ìñâ\r\n        this.$nextTick(() => {\r\n            const canvas = document.getElementById(canvasId);\r\n\r\n            // üö® canvasÍ∞Ä Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏúºÎ©¥ ÏóêÎü¨ Î∞©ÏßÄ\r\n            if (!canvas) {\r\n                console.error(`Canvas element not found: ${canvasId}`);\r\n                return;\r\n            }\r\n\r\n            const ctx = canvas.getContext('2d');\r\n\r\n            // Í∏∞Ï°¥ Ï∞®Ìä∏Í∞Ä ÏûàÏúºÎ©¥ ÏÇ≠Ï†ú\r\n            if (this.chartInstances && this.chartInstances[ts.id.tsName]) {\r\n                this.chartInstances[ts.id.tsName].destroy();\r\n            }\r\n\r\n            // chartInstances Í∞ùÏ≤¥ Ï¥àÍ∏∞Ìôî\r\n            if (!this.chartInstances) {\r\n                this.chartInstances = {};\r\n            }\r\n\r\n            // üî¢ ÏÇ¨Ïö©Î•† Î∞òÏò¨Î¶º (ÏÜåÏàòÏ†ê 1ÏûêÎ¶¨)\r\n            const roundedUsedRate = parseFloat(ts.usedRate.toFixed(1));\r\n\r\n            // ÏÉà Ï∞®Ìä∏ ÏÉùÏÑ±\r\n            this.chartInstances[ts.id.tsName] = new Chart(ctx, {\r\n                type: 'bar',\r\n                data: {\r\n                    labels: ['ÏÇ¨Ïö©Î•†'],\r\n                    datasets: [{\r\n                        label: 'ÏÇ¨Ïö©Î•†',\r\n                        data: [roundedUsedRate], // Î∞òÏò¨Î¶ºÎêú Í∞í Ï†ÅÏö©\r\n                        backgroundColor: 'rgba(75, 192, 192, 0.2)',\r\n                        borderColor: 'rgb(75, 192, 192)',\r\n                        borderRadius: 1,\r\n                        borderWidth: 1,\r\n                    }],\r\n                },\r\n                options: {\r\n                    responsive: true,\r\n                    indexAxis: 'y',\r\n                    scales: {\r\n                        x: {\r\n                            min: 0,\r\n                            max: 100,\r\n                            ticks: { display: false }, // XÏ∂ï ÎààÍ∏à Ïà®ÍπÄ\r\n                            grid: { display: false }, // XÏ∂ï Í∑∏Î¶¨Îìú ÏÑ† Ïà®ÍπÄ\r\n                            border: { display: false }, // XÏ∂ï Í≤ΩÍ≥ÑÏÑ† Ïà®ÍπÄ\r\n                        },\r\n                        y: {\r\n                            beginAtZero: true,\r\n                            display: false, // YÏ∂ï Ïà®ÍπÄ\r\n                            grid: { display: true }, // YÏ∂ï Í∑∏Î¶¨Îìú ÏÑ† Ïà®ÍπÄ\r\n                        },\r\n                    },\r\n                    plugins: {\r\n                        legend: { display: false }, // Î≤îÎ°Ä Ïà®ÍπÄ\r\n                        datalabels: {\r\n                            display: true,\r\n                            align: (ctx) => ctx.dataset.data[0] >= 56 ? 'center' : 'end',\r\n                            anchor: (ctx) => ctx.dataset.data[0] >= 56 ? 'center' : 'end',\r\n                            formatter: (value) => `${value.toFixed(1)}%`, // üî¢ ÏÜåÏàòÏ†ê 1ÏûêÎ¶¨ Ï†ÅÏö©\r\n                            color: (ctx) => ctx.dataset.data[0] >= 85 ? 'red' : 'rgb(75, 192, 192)',\r\n                            font: { weight: 'bold', size: 14 },\r\n                        },\r\n                    },\r\n                },\r\n                plugins: [ChartDataLabels],\r\n            });\r\n        });\r\n    }\r\n\r\n\r\n  }\r\n,\r\n\r\n  mounted() {\r\n    this.fetchDbList();\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n/* Í∏∞Î≥∏ ÏÑ§Ï†ï */\r\n.container {\r\n  font-family: 'Arial', sans-serif;\r\n  padding: 40px;\r\n  max-width: 1000px;\r\n  margin: 0 auto;\r\n  background-color: #ffffff;\r\n  border-radius: 12px;\r\n  box-shadow: 0 6px 15px rgba(0, 0, 0, 0.1);\r\n  transition: 0.3s;\r\n}\r\n\r\n.container:hover {\r\n  box-shadow: 0 8px 18px rgba(0, 0, 0, 0.2); /* ÎßàÏö∞Ïä§ Ïò§Î≤Ñ Ïãú Í∑∏Î¶ºÏûê Ìö®Í≥º */\r\n}\r\n\r\nh2 {\r\n  color: #333;\r\n  text-align: center;\r\n  font-size: 28px;\r\n  margin-bottom: 30px;\r\n  font-weight: 600;\r\n  letter-spacing: 0.5px;\r\n  transition: color 0.3s;\r\n}\r\n\r\nh2:hover {\r\n  color: #4caf50; /* ÎßàÏö∞Ïä§ Ïò§Î≤Ñ Ïãú ÏÉâÏÉÅ Î≥ÄÌôî */\r\n}\r\n\r\n/* ÎìúÎ°≠Îã§Ïö¥ Ïä§ÌÉÄÏùº */\r\n.select-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\nselect {\r\n  padding: 12px 18px;\r\n  font-size: 16px;\r\n  border-radius: 8px;\r\n  border: 