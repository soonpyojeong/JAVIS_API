{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      smsHistories: [],\n      filteredSmsHistories: [],\n      day: 3,\n      // Í∏∞Î≥∏Í∞í\n      msgSearch: '',\n      showModal: false,\n      updateMessage: ''\n    };\n  },\n  mounted() {\n    this.fetchSmsHistories();\n  },\n  methods: {\n    methods: {\n      fetchSmsHistories() {\n        // day Í∞íÏù¥ Ï†úÎåÄÎ°ú Îì§Ïñ¥Ïò§ÎäîÏßÄ ÌôïÏù∏\n        console.log(\"Ï°∞ÌöåÌï† ÏùºÏàò(day) Í∞í:\", this.day);\n        if (this.day && this.day > 0) {\n          // day Í∞íÏù¥ 1 Ïù¥ÏÉÅÏùº ÎïåÎßå API ÏöîÏ≤≠\n          axios.get(`/api/sms/all?day=${this.day}`).then(response => {\n            this.smsHistories = response.data;\n            // APIÏóêÏÑú Î∞òÌôòÎêú Ï†ÑÏ≤¥ response Ï∂úÎ†•\n            console.log(\"APIÏóêÏÑú Î∞òÌôòÎêú Ï†ÑÏ≤¥ ÏùëÎãµ:\", response);\n\n            // Ïã§Ï†ú Îç∞Ïù¥ÌÑ∞Îäî response.data ÏïàÏóê ÏûàÏäµÎãàÎã§.\n            console.log(\"APIÏóêÏÑú Î∞òÌôòÎêú Îç∞Ïù¥ÌÑ∞:\", response.data);\n\n            // inDateÏôÄ inTimeÏùÑ Ï°∞Ìï©ÌïòÏó¨ ÎÇ†Ïßú ÎπÑÍµê\n            this.smsHistories.sort((a, b) => {\n              // inDateÏôÄ inTimeÏùÑ Í≤∞Ìï©ÌïòÏó¨ ÎπÑÍµê Í∞ÄÎä•Ìïú Î¨∏ÏûêÏó¥Î°ú ÎßåÎì§Í∏∞\n              const dateA = a.inDate + a.inTime; // inDateÏôÄ inTimeÏùÑ Ìï©Ï≥êÏÑú ÎπÑÍµê\n              const dateB = b.inDate + b.inTime;\n\n              // ÎÇ¥Î¶ºÏ∞®Ïàú Ï†ïÎ†¨ (Í∞ÄÏû• ÏµúÏã† ÎÇ†ÏßúÍ∞Ä Îß® ÏúÑÎ°ú Ïò§ÎèÑÎ°ù)\n              return dateB.localeCompare(dateA); // Î¨∏ÏûêÏó¥ ÎπÑÍµê\n            });\n            this.filteredSmsHistories = this.smsHistories; // ÌïÑÌÑ∞ÎßÅÎêú Î™©Î°ù Í∞±Ïã†\n          }).catch(error => {\n            console.error('Error fetching SMS histories:', error);\n          });\n        } else {\n          // day Í∞íÏù¥ Ïò¨Î∞îÎ•¥ÏßÄ ÏïäÏúºÎ©¥ Í∏∞Î≥∏ Í∞í Ï≤òÎ¶¨ ÎòêÎäî Í≤ΩÍ≥†\n          console.log('Ï°∞ÌöåÌï† ÏùºÏàòÍ∞Ä Ïò¨Î∞îÎ•¥ÏßÄ ÏïäÏäµÎãàÎã§.');\n          this.smsHistories = [];\n          this.filteredSmsHistories = [];\n        }\n      },\n      // Î©îÏãúÏßÄ ÎÇ¥Ïö© ÌïÑÌÑ∞ÎßÅ Ìï®Ïàò\n      filterSmsHistories() {\n        // msgSearchÍ∞Ä ÏûàÏùÑ Í≤ΩÏö∞ ÌïÑÌÑ∞ÎßÅ\n        this.filteredSmsHistories = this.msgSearch ? this.smsHistories.filter(history => history.msg && history.msg.includes(this.msgSearch)) : this.smsHistories;\n      },\n      // ÎåÄÎüâ Î©îÏãúÏßÄ Ï†ÑÏÜ° Ï≤òÎ¶¨\n      updateAllSmsHistories() {\n        axios.put('/api/sms/updateall').then(response => {\n          const updatedCount = response.data?.updatedCount || 0;\n          this.updateMessage = `Ï†ÑÏ≤¥ Î©îÏãúÏßÄ Ï†ÑÏÜ° Ï≤òÎ¶¨Í∞Ä ÏôÑÎ£åÎêòÏóàÏäµÎãàÎã§. (Ï¥ù ${updatedCount}Í±¥)`;\n          this.showModal = true;\n          this.fetchSmsHistories(); // ÏôÑÎ£å ÌõÑ Îã§Ïãú Ï°∞Ìöå\n        }).catch(error => {\n          console.error('Error updating SMS histories:', error);\n        });\n      }\n    },\n    filterSmsHistories() {\n      // msgSearchÍ∞Ä ÏûàÏùÑ Í≤ΩÏö∞ ÌïÑÌÑ∞ÎßÅ\n      this.filteredSmsHistories = this.msgSearch ? this.smsHistories.filter(history => history.msg && history.msg.includes(this.msgSearch)) : this.smsHistories;\n    },\n    updateAllSmsHistories() {\n      axios.put('/api/sms/updateall').then(response => {\n        const updatedCount = response.data?.updatedCount || 0;\n        this.updateMessage = `Ï†ÑÏ≤¥ Î©îÏãúÏßÄ Ï†ÑÏÜ° Ï≤òÎ¶¨Í∞Ä ÏôÑÎ£åÎêòÏóàÏäµÎãàÎã§. (Ï¥ù ${updatedCount}Í±¥)`;\n        this.showModal = true;\n        this.fetchSmsHistories(); // ÏôÑÎ£å ÌõÑ Îã§Ïãú Ï°∞Ìöå\n      }).catch(error => {\n        console.error('Error updating SMS histories:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","smsHistories","filteredSmsHistories","day","msgSearch","showModal","updateMessage","mounted","fetchSmsHistories","methods","console","log","get","then","response","sort","a","b","dateA","inDate","inTime","dateB","localeCompare","catch","error","filterSmsHistories","filter","history","msg","includes","updateAllSmsHistories","put","updatedCount"],"sources":["D:\\JAVIS\\dongkukDBmon\\dongkukDBmon\\frontend\\src\\components\\SmsHistory.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h1 class=\"title\">üì® SMS Ï†ÑÏÜ° ÌûàÏä§ÌÜ†Î¶¨</h1>\r\n\r\n    <div class=\"control-box\">\r\n      <!-- Ï°∞ÌöåÏùºÏàò + Ï°∞Ìöå Î≤ÑÌäº Ìïú Ï§Ñ Ï†ïÎ†¨ -->\r\n      <div class=\"row-group\">\r\n        <label for=\"days\">Ï°∞ÌöåÌï† ÏùºÏàò</label>\r\n        <input type=\"number\" id=\"days\" v-model=\"day\" min=\"1\" />\r\n        <button @click=\"fetchSmsHistories\">Ï°∞Ìöå</button>\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"msgSearch\">MSG ÎÇ¥Ïö© Í≤ÄÏÉâ</label>\r\n        <input type=\"text\" id=\"msgSearch\" v-model=\"msgSearch\" @input=\"filterSmsHistories\" />\r\n      </div>\r\n\r\n      <div class=\"button-group\">\r\n        <button @click=\"updateAllSmsHistories\" class=\"orange\" title=\"Î™ÖÏ†àÏù¥ÎÇò Ïû•ÏãúÍ∞Ñ Í¥ÄÏ†ú Ï§ëÏßÄÏãú ÏÇ¨Ïö©!!\">ÎåÄÎüâ Î©îÏãúÏßÄ Ï†ÑÏÜ° Ï≤òÎ¶¨</button>\r\n      </div>\r\n    </div>\r\n\r\n    <table>\r\n      <thead>\r\n        <tr>\r\n          <th>SEQNO</th>\r\n          <th>INDATE</th>\r\n          <th>INTIME</th>\r\n          <th>SENDNAME</th>\r\n          <th>RECVNAME</th>\r\n          <th>MSG</th>\r\n          <th>RESULT</th>\r\n          <th>KIND</th>\r\n          <th>ERRCODE</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"history in filteredSmsHistories\" :key=\"history.SEQNO\">\r\n          <td>{{ history.SEQNO }}</td>\r\n          <td>{{ history.inDate }}</td>\r\n          <td>{{ history.inTime }}</td>\r\n          <td>{{ history.sendName }}</td>\r\n          <td>{{ history.recName }}</td>\r\n          <td>{{ history.msg }}</td>\r\n          <td>{{ history.result }}</td>\r\n          <td>{{ history.kind }}</td>\r\n          <td>{{ history.errCode }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Î™®Îã¨ ÌåùÏóÖ -->\r\n    <div v-if=\"showModal\" class=\"modal-overlay\">\r\n      <div class=\"modal\">\r\n        <p>‚úÖ {{ updateMessage }}</p>\r\n        <button @click=\"showModal = false\">ÌôïÏù∏</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      smsHistories: [],\r\n      filteredSmsHistories: [],\r\n      day: 3,  // Í∏∞Î≥∏Í∞í\r\n      msgSearch: '',\r\n      showModal: false,\r\n      updateMessage: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchSmsHistories();\r\n  },\r\n  methods: {\r\n    methods: {\r\n      fetchSmsHistories() {\r\n        // day Í∞íÏù¥ Ï†úÎåÄÎ°ú Îì§Ïñ¥Ïò§ÎäîÏßÄ ÌôïÏù∏\r\n        console.log(\"Ï°∞ÌöåÌï† ÏùºÏàò(day) Í∞í:\", this.day);\r\n\r\n        if (this.day && this.day > 0) {  // day Í∞íÏù¥ 1 Ïù¥ÏÉÅÏùº ÎïåÎßå API ÏöîÏ≤≠\r\n          axios.get(`/api/sms/all?day=${this.day}`)\r\n            .then(response => {\r\n              this.smsHistories = response.data;\r\n              // APIÏóêÏÑú Î∞òÌôòÎêú Ï†ÑÏ≤¥ response Ï∂úÎ†•\r\n              console.log(\"APIÏóêÏÑú Î∞òÌôòÎêú Ï†ÑÏ≤¥ ÏùëÎãµ:\", response);\r\n\r\n              // Ïã§Ï†ú Îç∞Ïù¥ÌÑ∞Îäî response.data ÏïàÏóê ÏûàÏäµÎãàÎã§.\r\n              console.log(\"APIÏóêÏÑú Î∞òÌôòÎêú Îç∞Ïù¥ÌÑ∞:\", response.data);\r\n\r\n              // inDateÏôÄ inTimeÏùÑ Ï°∞Ìï©ÌïòÏó¨ ÎÇ†Ïßú ÎπÑÍµê\r\n              this.smsHistories.sort((a, b) => {\r\n                // inDateÏôÄ inTimeÏùÑ Í≤∞Ìï©ÌïòÏó¨ ÎπÑÍµê Í∞ÄÎä•Ìïú Î¨∏ÏûêÏó¥Î°ú ÎßåÎì§Í∏∞\r\n                const dateA = a.inDate + a.inTime;  // inDateÏôÄ inTimeÏùÑ Ìï©Ï≥êÏÑú ÎπÑÍµê\r\n                const dateB = b.inDate + b.inTime;\r\n\r\n                // ÎÇ¥Î¶ºÏ∞®Ïàú Ï†ïÎ†¨ (Í∞ÄÏû• ÏµúÏã† ÎÇ†ÏßúÍ∞Ä Îß® ÏúÑÎ°ú Ïò§ÎèÑÎ°ù)\r\n                return dateB.localeCompare(dateA);  // Î¨∏ÏûêÏó¥ ÎπÑÍµê\r\n              });\r\n\r\n              this.filteredSmsHistories = this.smsHistories; // ÌïÑÌÑ∞ÎßÅÎêú Î™©Î°ù Í∞±Ïã†\r\n            })\r\n            .catch(error => {\r\n              console.error('Error fetching SMS histories:', error);\r\n            });\r\n        } else {\r\n          // day Í∞íÏù¥ Ïò¨Î∞îÎ•¥ÏßÄ ÏïäÏúºÎ©¥ Í∏∞Î≥∏ Í∞í Ï≤òÎ¶¨ ÎòêÎäî Í≤ΩÍ≥†\r\n          console.log('Ï°∞ÌöåÌï† ÏùºÏàòÍ∞Ä Ïò¨Î∞îÎ•¥ÏßÄ ÏïäÏäµÎãàÎã§.');\r\n          this.smsHistories = [];\r\n          this.filteredSmsHistories = [];\r\n        }\r\n      },\r\n\r\n      // Î©îÏãúÏßÄ ÎÇ¥Ïö© ÌïÑÌÑ∞ÎßÅ Ìï®Ïàò\r\n      filterSmsHistories() {\r\n        // msgSearchÍ∞Ä ÏûàÏùÑ Í≤ΩÏö∞ ÌïÑÌÑ∞ÎßÅ\r\n        this.filteredSmsHistories = this.msgSearch\r\n          ? this.smsHistories.filter(history =>\r\n              history.msg && history.msg.includes(this.msgSearch))\r\n          : this.smsHistories;\r\n      },\r\n\r\n      // ÎåÄÎüâ Î©îÏãúÏßÄ Ï†ÑÏÜ° Ï≤òÎ¶¨\r\n      updateAllSmsHistories() {\r\n        axios.put('/api/sms/updateall')\r\n          .then(response => {\r\n            const updatedCount = response.data?.updatedCount || 0;\r\n            this.updateMessage = `Ï†ÑÏ≤¥ Î©îÏãúÏßÄ Ï†ÑÏÜ° Ï≤òÎ¶¨Í∞Ä ÏôÑÎ£åÎêòÏóàÏäµÎãàÎã§. (Ï¥ù ${updatedCount}Í±¥)`;\r\n            this.showModal = true;\r\n            this.fetchSmsHistories();  // ÏôÑÎ£å ÌõÑ Îã§Ïãú Ï°∞Ìöå\r\n          })\r\n          .catch(error => {\r\n            console.error('Error updating SMS histories:', error);\r\n          });\r\n      },\r\n    }\r\n,\r\n    filterSmsHistories() {\r\n      // msgSearchÍ∞Ä ÏûàÏùÑ Í≤ΩÏö∞ ÌïÑÌÑ∞ÎßÅ\r\n      this.filteredSmsHistories = this.msgSearch\r\n        ? this.smsHistories.filter(history =>\r\n            history.msg && history.msg.includes(this.msgSearch))\r\n        : this.smsHistories;\r\n    },\r\n    updateAllSmsHistories() {\r\n      axios.put('/api/sms/updateall')\r\n        .then(response => {\r\n          const updatedCount = response.data?.updatedCount || 0;\r\n          this.updateMessage = `Ï†ÑÏ≤¥ Î©îÏãúÏßÄ Ï†ÑÏÜ° Ï≤òÎ¶¨Í∞Ä ÏôÑÎ£åÎêòÏóàÏäµÎãàÎã§. (Ï¥ù ${updatedCount}Í±¥)`;\r\n          this.showModal = true;\r\n          this.fetchSmsHistories();  // ÏôÑÎ£å ÌõÑ Îã§Ïãú Ï°∞Ìöå\r\n        })\r\n        .catch(error => {\r\n          console.error('Error updating SMS histories:', error);\r\n        });\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Í∏∞Ï°¥ Ïä§ÌÉÄÏùº */\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 30px;\r\n  font-family: 'Segoe UI', sans-serif;\r\n}\r\n\r\n.title {\r\n  font-size: 26px;\r\n  font-weight: bold;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.control-box {\r\n  display: flex;\r\n  align-items: flex-end;\r\n  gap: 20px;\r\n  flex-wrap: wrap;\r\n  margin-bottom: 20px;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 8px;\r\n  background-color: #fafafa;\r\n}\r\n\r\n.form-group {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.row-group {\r\n  display: flex;\r\n  align-items: flex-end;\r\n  gap: 10px;\r\n  flex-wrap: wrap;\r\n}\r\n\r\ninput[type=\"number\"],\r\ninput[type=\"text\"] {\r\n  padding: 8px;\r\n  width: 200px;\r\n  border-radius: 4px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.button-group {\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px 16px;\r\n  border: none;\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-weight: bold;\r\n}\r\n\r\nbutton.orange {\r\n  background-color: #f57c00;\r\n}\r\n\r\nbutton:hover {\r\n  opacity: 0.9;\r\n}\r\n\r\nbutton:disabled {\r\n  background-color: #ccc;\r\n  cursor: not-allowed;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n  font-size: 14px;\r\n}\r\n\r\nth {\r\n  background-color: #f2f2f2;\r\n  padding: 10px;\r\n}\r\n\r\ntd {\r\n  padding: 10px;\r\n  text-align: center;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  align-items: center;\r\n 