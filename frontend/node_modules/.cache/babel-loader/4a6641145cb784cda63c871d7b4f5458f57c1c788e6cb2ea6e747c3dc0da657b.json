{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nexport default {\n  data() {\n    return {\n      monitoringItems: [{\n        title: '테이블스페이스 관제',\n        events: [{\n          id: 1,\n          message: '테이블스페이스 80% 초과 경고'\n        }],\n        style: {\n          position: 'absolute',\n          top: '20px',\n          left: '20px',\n          zIndex: 1,\n          width: '200px',\n          height: '150px'\n        }\n      }, {\n        title: '생사 관제',\n        events: [],\n        style: {\n          position: 'absolute',\n          top: '20px',\n          left: '300px',\n          zIndex: 1,\n          width: '200px',\n          height: '150px'\n        }\n      }, {\n        title: '오브젝트 인밸리드 관제',\n        events: [{\n          id: 3,\n          message: 'Invalid Object 발견: FUNCTION_A'\n        }],\n        style: {\n          position: 'absolute',\n          top: '20px',\n          left: '580px',\n          zIndex: 1,\n          width: '200px',\n          height: '150px'\n        }\n      }, {\n        title: '락 리스트 관제',\n        events: [{\n          id: 4,\n          message: 'Deadlock 발생: Session 101'\n        }],\n        style: {\n          position: 'absolute',\n          top: '300px',\n          left: '20px',\n          zIndex: 1,\n          width: '200px',\n          height: '150px'\n        }\n      }],\n      scheduleTasks: [{\n        id: 1,\n        name: 'DB 백업',\n        completed: false,\n        email: ''\n      }, {\n        id: 2,\n        name: '서버 점검',\n        completed: false,\n        email: ''\n      }, {\n        id: 3,\n        name: '로그 분석',\n        completed: false,\n        email: ''\n      }],\n      dragData: {\n        isDragging: false,\n        index: null,\n        offsetX: 0,\n        offsetY: 0,\n        isResizing: false,\n        // 크기 조정 상태\n        resizeDirection: '' // 크기 조정 방향\n      }\n    };\n  },\n  methods: {\n    sendEmails() {\n      const completedTasks = this.scheduleTasks.filter(task => task.completed);\n      completedTasks.forEach(task => {\n        if (task.email) {\n          console.log(`이메일 발송: ${task.email}, 작업: ${task.name}`);\n          // 실제 이메일 발송 로직 추가 필요\n        } else {\n          console.log(`작업자 이메일이 설정되지 않았습니다: 작업 ${task.name}`);\n        }\n      });\n      alert('이메일 발송이 완료되었습니다.');\n    },\n    startDrag(event, index) {\n      if (event.target.classList.contains('resize-handle')) {\n        // 크기 조정 핸들 클릭 시 크기 조정 시작\n        this.dragData.isResizing = true;\n        this.dragData.resizeDirection = event.target.dataset.direction;\n        return;\n      }\n\n      // 기존의 드래그 코드\n      this.dragData.isDragging = true;\n      this.dragData.index = index;\n      this.dragData.offsetX = event.clientX - parseInt(this.monitoringItems[index].style.left);\n      this.dragData.offsetY = event.clientY - parseInt(this.monitoringItems[index].style.top);\n      this.monitoringItems[index].style.zIndex = 1000;\n      window.addEventListener('mousemove', this.onDrag);\n      window.addEventListener('mouseup', this.stopDrag);\n    },\n    onDrag(event) {\n      const item = this.monitoringItems[this.dragData.index];\n      if (this.dragData.isResizing) {\n        // 크기 조정 로직\n        if (this.dragData.resizeDirection === 'bottom-right') {\n          const newWidth = Math.max(100, event.clientX - parseInt(item.style.left));\n          const newHeight = Math.max(100, event.clientY - parseInt(item.style.top));\n          item.style.width = `${newWidth}px`;\n          item.style.height = `${newHeight}px`;\n        }\n      } else if (this.dragData.isDragging) {\n        // 이동 로직\n        const newLeft = event.clientX - this.dragData.offsetX;\n        const newTop = event.clientY - this.dragData.offsetY;\n        item.style.left = `${Math.max(0, Math.min(newLeft, window.innerWidth - item.offsetWidth))}px`;\n        item.style.top = `${Math.max(0, Math.min(newTop, window.innerHeight - item.offsetHeight))}px`;\n      }\n    },\n    stopDrag() {\n      if (this.dragData.isDragging || this.dragData.isResizing) {\n        this.monitoringItems[this.dragData.index].style.zIndex = 1;\n        this.dragData.isDragging = false;\n        this.dragData.isResizing = false;\n        window.removeEventListener('mousemove', this.onDrag);\n        window.removeEventListener('mouseup', this.stopDrag);\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","monitoringItems","title","events","id","message","style","position","top","left","zIndex","width","height","scheduleTasks","name","completed","email","dragData","isDragging","index","offsetX","offsetY","isResizing","resizeDirection","methods","sendEmails","completedTasks","filter","task","forEach","console","log","alert","startDrag","event","target","classList","contains","dataset","direction","clientX","parseInt","clientY","window","addEventListener","onDrag","stopDrag","item","newWidth","Math","max","newHeight","newLeft","newTop","min","innerWidth","offsetWidth","innerHeight","offsetHeight","removeEventListener"],"sources":["D:\\JAVIS\\dongkukDBmon\\dongkukDBmon\\frontend\\src\\components\\MainPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard\">\r\n    <div\r\n      class=\"monitoring-box draggable\"\r\n      v-for=\"(item, index) in monitoringItems\"\r\n      :key=\"item.title\"\r\n      :style=\"item.style\"\r\n      @mousedown=\"startDrag($event, index)\"\r\n    >\r\n      <h2>{{ item.title }}</h2>\r\n      <ul>\r\n        <li v-for=\"event in item.events\" :key=\"event.id\">\r\n          {{ event.message }}\r\n        </li>\r\n      </ul>\r\n      <div v-if=\"item.events.length === 0\" class=\"no-events\">현재 이벤트가 없습니다.</div>\r\n\r\n      <!-- 크기 조정 핸들 추가 -->\r\n      <div class=\"resize-handle\" data-direction=\"bottom-right\"></div>\r\n    </div>\r\n\r\n    <div class=\"schedule-box\">\r\n      <h2>고정 일정 관리</h2>\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th>작업 항목</th>\r\n            <th>완료 여부</th>\r\n            <th>작업자 이메일</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"task in scheduleTasks\" :key=\"task.id\">\r\n            <td>{{ task.name }}</td>\r\n            <td>\r\n              <input type=\"checkbox\" v-model=\"task.completed\" />\r\n            </td>\r\n            <td>\r\n              <input type=\"email\" v-model=\"task.email\" placeholder=\"작업자 이메일 입력\" />\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <button @click=\"sendEmails\">이메일 발송</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      monitoringItems: [\r\n        {\r\n          title: '테이블스페이스 관제',\r\n          events: [{ id: 1, message: '테이블스페이스 80% 초과 경고' }],\r\n          style: { position: 'absolute', top: '20px', left: '20px', zIndex: 1, width: '200px', height: '150px' },\r\n        },\r\n        {\r\n          title: '생사 관제',\r\n          events: [],\r\n          style: { position: 'absolute', top: '20px', left: '300px', zIndex: 1, width: '200px', height: '150px' },\r\n        },\r\n        {\r\n          title: '오브젝트 인밸리드 관제',\r\n          events: [{ id: 3, message: 'Invalid Object 발견: FUNCTION_A' }],\r\n          style: { position: 'absolute', top: '20px', left: '580px', zIndex: 1, width: '200px', height: '150px' },\r\n        },\r\n        {\r\n          title: '락 리스트 관제',\r\n          events: [{ id: 4, message: 'Deadlock 발생: Session 101' }],\r\n          style: { position: 'absolute', top: '300px', left: '20px', zIndex: 1, width: '200px', height: '150px' },\r\n        },\r\n      ],\r\n      scheduleTasks: [\r\n        { id: 1, name: 'DB 백업', completed: false, email: '' },\r\n        { id: 2, name: '서버 점검', completed: false, email: '' },\r\n        { id: 3, name: '로그 분석', completed: false, email: '' },\r\n      ],\r\n      dragData: {\r\n        isDragging: false,\r\n        index: null,\r\n        offsetX: 0,\r\n        offsetY: 0,\r\n        isResizing: false, // 크기 조정 상태\r\n        resizeDirection: '', // 크기 조정 방향\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    sendEmails() {\r\n      const completedTasks = this.scheduleTasks.filter(task => task.completed);\r\n      completedTasks.forEach(task => {\r\n        if (task.email) {\r\n          console.log(`이메일 발송: ${task.email}, 작업: ${task.name}`);\r\n          // 실제 이메일 발송 로직 추가 필요\r\n        } else {\r\n          console.log(`작업자 이메일이 설정되지 않았습니다: 작업 ${task.name}`);\r\n        }\r\n      });\r\n      alert('이메일 발송이 완료되었습니다.');\r\n    },\r\n    startDrag(event, index) {\r\n      if (event.target.classList.contains('resize-handle')) {\r\n        // 크기 조정 핸들 클릭 시 크기 조정 시작\r\n        this.dragData.isResizing = true;\r\n        this.dragData.resizeDirection = event.target.dataset.direction;\r\n        return;\r\n      }\r\n\r\n      // 기존의 드래그 코드\r\n      this.dragData.isDragging = true;\r\n      this.dragData.index = index;\r\n      this.dragData.offsetX = event.clientX - parseInt(this.monitoringItems[index].style.left);\r\n      this.dragData.offsetY = event.clientY - parseInt(this.monitoringItems[index].style.top);\r\n\r\n      this.monitoringItems[index].style.zIndex = 1000;\r\n\r\n      window.addEventListener('mousemove', this.onDrag);\r\n      window.addEventListener('mouseup', this.stopDrag);\r\n    },\r\n    onDrag(event) {\r\n      const item = this.monitoringItems[this.dragData.index];\r\n      if (this.dragData.isResizing) {\r\n        // 크기 조정 로직\r\n        if (this.dragData.resizeDirection === 'bottom-right') {\r\n          const newWidth = Math.max(100, event.clientX - parseInt(item.style.left));\r\n          const newHeight = Math.max(100, event.clientY - parseInt(item.style.top));\r\n          item.style.width = `${newWidth}px`;\r\n          item.style.height = `${newHeight}px`;\r\n        }\r\n      } else if (this.dragData.isDragging) {\r\n        // 이동 로직\r\n        const newLeft = event.clientX - this.dragData.offsetX;\r\n        const newTop = event.clientY - this.dragData.offsetY;\r\n        item.style.left = `${Math.max(0, Math.min(newLeft, window.innerWidth - item.offsetWidth))}px`;\r\n        item.style.top = `${Math.max(0, Math.min(newTop, window.innerHeight - item.offsetHeight))}px`;\r\n      }\r\n    },\r\n    stopDrag() {\r\n      if (this.dragData.isDragging || this.dragData.isResizing) {\r\n        this.monitoringItems[this.dragData.index].style.zIndex = 1;\r\n        this.dragData.isDragging = false;\r\n        this.dragData.isResizing = false;\r\n\r\n        window.removeEventListener('mousemove', this.onDrag);\r\n        window.removeEventListener('mouseup', this.stopDrag);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.dashboard {\r\n  position: relative;\r\n  height: 100vh;\r\n  background: #f9f9f9;\r\n  font-family: 'Arial', sans-serif;\r\n  overflow: hidden;\r\n}\r\n\r\n.monitoring-box, .schedule-box {\r\n  background: white;\r\n  user-select: none;\r\n  border: 1px solid #ddd;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  padding: 15px;\r\n  cursor: grab;\r\n  transition: transform 0.2s ease, left 0.2s ease, top 0.2s ease, width 0.2s ease, height 0.2s ease;\r\n  position: relative; /* Added position for resize handle */\r\n}\r\n\r\n.monitoring-box:active {\r\n  cursor: grabbing;\r\n}\r\n\r\n/* Resize handle */\r\n.monitoring-box .resize-handle {\r\n  width: 15px;\r\n  height: 15px;\r\n  background-color: #ccc;\r\n  position: absolute;\r\n  right: 0;\r\n  bottom: 0;\r\n  cursor: se-resize;\r\n}\r\n\r\n.monitoring-box:active .resize-handle {\r\n  background-color: #666;\r\n}\r\n\r\n.monitoring-box h2, .schedule-box h2 {\r\n  font-size: 18px;\r\n  margin-bottom: 10px;\r\n  color: #333;\r\n  border-bottom: 1px solid #ddd;\r\n  padding-bottom: 5px;\r\n}\r\n\r\n.monitoring-box ul {\r\n  list-style: none;\r\n  padding: 0;\