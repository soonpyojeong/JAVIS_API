{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, onUnmounted } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nexport default {\n  setup() {\n    const router = useRouter();\n    const route = useRoute();\n    const menuTop = ref(25); // 메뉴의 현재 위치\n    const selectedMenu = ref(route.path); // 현재 선택된 메뉴\n\n    const handleScroll = () => {\n      const offsetTop = 10; // 초기 상단 여백\n      menuTop.value = Math.max(offsetTop, window.scrollY); // 스크롤에 따라 top 값 변경\n    };\n    const navigateTo = path => {\n      selectedMenu.value = path; // 선택된 메뉴 경로 업데이트\n      router.push(path);\n    };\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll);\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll);\n    });\n    const menuItems = [{\n      name: '첫화면',\n      path: '/'\n    }, {\n      name: 'DB 전체 리스트',\n      path: '/db-list'\n    }, {\n      name: 'SMS 전송 내역',\n      path: '/sms-history'\n    }, {\n      name: '임계치 리스트',\n      path: '/threshold-list'\n    }, {\n      name: '테이블스페이스 리스트',\n      path: '/tablespaces'\n    }, {\n      name: '일일 점검(hit율)',\n      path: '/dailyChk'\n    }];\n    return {\n      navigateTo,\n      menuTop,\n      menuItems,\n      selectedMenu\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","useRouter","useRoute","setup","router","route","menuTop","selectedMenu","path","handleScroll","offsetTop","value","Math","max","window","scrollY","navigateTo","push","addEventListener","removeEventListener","menuItems","name"],"sources":["D:\\JAVIS\\dongkukDBmon\\dongkukDBmon\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"left-menu\" :style=\"{ top: `${menuTop}px` }\">\n      <ul>\n        <li\n          v-for=\"item in menuItems\"\n          :key=\"item.path\"\n          :class=\"{ active: selectedMenu === item.path }\"\n          @click=\"navigateTo(item.path)\"\n        >\n          {{ item.name }}\n        </li>\n      </ul>\n    </div>\n    <div class=\"right-content\">\n      <router-view /> <!-- 라우팅된 컴포넌트를 렌더링 -->\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted, onUnmounted } from 'vue'\nimport { useRouter, useRoute } from 'vue-router'\n\nexport default {\n  setup() {\n    const router = useRouter()\n    const route = useRoute()\n    const menuTop = ref(25) // 메뉴의 현재 위치\n    const selectedMenu = ref(route.path) // 현재 선택된 메뉴\n\n    const handleScroll = () => {\n      const offsetTop = 10 // 초기 상단 여백\n      menuTop.value = Math.max(offsetTop, window.scrollY) // 스크롤에 따라 top 값 변경\n    }\n\n    const navigateTo = (path) => {\n      selectedMenu.value = path // 선택된 메뉴 경로 업데이트\n      router.push(path)\n    }\n\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll)\n    })\n\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll)\n    })\n\n    const menuItems = [\n      { name: '첫화면', path: '/' },\n      { name: 'DB 전체 리스트', path: '/db-list' },\n      { name: 'SMS 전송 내역', path: '/sms-history' },\n      { name: '임계치 리스트', path: '/threshold-list' },\n      { name: '테이블스페이스 리스트', path: '/tablespaces' },\n      { name: '일일 점검(hit율)', path: '/dailyChk' }\n    ]\n\n    return { navigateTo, menuTop, menuItems, selectedMenu }\n  }\n}\n</script>\n\n<style>\n#app {\n  display: flex;\n  height: 100%;\n}\n\n.left-menu {\n  width: 200px;\n  background: #f4f4f4;\n  padding: 20px;\n  position: absolute;\n  font-family: 'Arial', sans-serif;\n  left: 0;\n  border: 3px solid #90EE90;\n  background: #ffffff; /* 흰색 배경 */\n  border-radius: 10px;\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1); /* 그림자 강조 */\n}\n\n.left-menu ul {\n  list-style: none;\n  padding: 0;\n}\n\n.left-menu li {\n  padding: 10px;\n  cursor: pointer;\n  transition: background 0.3s ease;\n}\n\n.left-menu li:hover {\n  background: #ddd;\n}\n\n.left-menu li.active {\n  background: #add8e6; /* 파스텔 파랑색 */\n  color: #000; /* 텍스트 색상 조정 */\n  border-radius: 5px;\n}\n\n.right-content {\n  flex: 1;\n  padding: 20px;\n  margin-left: 220px;\n}\n</style>\n"],"mappings":";AAqBA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAU,QAAS,KAAI;AAChD,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAW;AAE/C,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIH,SAAS,CAAC;IACzB,MAAMI,KAAI,GAAIH,QAAQ,CAAC;IACvB,MAAMI,OAAM,GAAIR,GAAG,CAAC,EAAE,GAAE;IACxB,MAAMS,YAAW,GAAIT,GAAG,CAACO,KAAK,CAACG,IAAI,GAAE;;IAErC,MAAMC,YAAW,GAAIA,CAAA,KAAM;MACzB,MAAMC,SAAQ,GAAI,EAAC,EAAE;MACrBJ,OAAO,CAACK,KAAI,GAAIC,IAAI,CAACC,GAAG,CAACH,SAAS,EAAEI,MAAM,CAACC,OAAO,GAAE;IACtD;IAEA,MAAMC,UAAS,GAAKR,IAAI,IAAK;MAC3BD,YAAY,CAACI,KAAI,GAAIH,IAAG,EAAE;MAC1BJ,MAAM,CAACa,IAAI,CAACT,IAAI;IAClB;IAEAT,SAAS,CAAC,MAAM;MACde,MAAM,CAACI,gBAAgB,CAAC,QAAQ,EAAET,YAAY;IAChD,CAAC;IAEDT,WAAW,CAAC,MAAM;MAChBc,MAAM,CAACK,mBAAmB,CAAC,QAAQ,EAAEV,YAAY;IACnD,CAAC;IAED,MAAMW,SAAQ,GAAI,CAChB;MAAEC,IAAI,EAAE,KAAK;MAAEb,IAAI,EAAE;IAAI,CAAC,EAC1B;MAAEa,IAAI,EAAE,WAAW;MAAEb,IAAI,EAAE;IAAW,CAAC,EACvC;MAAEa,IAAI,EAAE,WAAW;MAAEb,IAAI,EAAE;IAAe,CAAC,EAC3C;MAAEa,IAAI,EAAE,SAAS;MAAEb,IAAI,EAAE;IAAkB,CAAC,EAC5C;MAAEa,IAAI,EAAE,aAAa;MAAEb,IAAI,EAAE;IAAe,CAAC,EAC7C;MAAEa,IAAI,EAAE,aAAa;MAAEb,IAAI,EAAE;IAAY,EAC3C;IAEA,OAAO;MAAEQ,UAAU;MAAEV,OAAO;MAAEc,SAAS;MAAEb;IAAa;EACxD;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}