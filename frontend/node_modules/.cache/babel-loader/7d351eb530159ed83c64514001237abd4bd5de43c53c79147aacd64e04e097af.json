{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { onMounted, onUnmounted, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const router = useRouter();\n    const menuTop = ref(0); // 메뉴의 `top` 위치 값\n\n    const handleScroll = () => {\n      menuTop.value = window.scrollY; // 현재 스크롤 위치를 반영\n    };\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll);\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll);\n    });\n    const navigateTo = path => {\n      router.push(path); // 클릭 시 해당 경로로 라우팅\n    };\n    return {\n      navigateTo,\n      menuTop\n    };\n  }\n};","map":{"version":3,"names":["onMounted","onUnmounted","ref","useRouter","setup","router","menuTop","handleScroll","value","window","scrollY","addEventListener","removeEventListener","navigateTo","path","push"],"sources":["D:\\JAVIS\\dongkukDBmon\\dongkukDBmon\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div\n      class=\"left-menu\"\n      :style=\"{ top: `${menuTop}px` }\"\n    >\n      <ul>\n        <li @click=\"navigateTo('/')\">첫화면</li>\n        <li @click=\"navigateTo('/db-list')\">DB 전체 리스트</li>\n        <li @click=\"navigateTo('/sms-history')\">SMS 전송 내역</li>\n        <li @click=\"navigateTo('/threshold-list')\">임계치 리스트</li>\n        <li @click=\"navigateTo('/tablespaces')\">TB 리스트</li>\n      </ul>\n    </div>\n    <div class=\"right-content\">\n      <router-view /> <!-- 라우팅된 컴포넌트를 렌더링 -->\n    </div>\n  </div>\n</template>\n\n<script>\nimport { onMounted, onUnmounted, ref } from 'vue'\nimport { useRouter } from 'vue-router'\n\nexport default {\n  setup() {\n    const router = useRouter()\n    const menuTop = ref(0) // 메뉴의 `top` 위치 값\n\n    const handleScroll = () => {\n      menuTop.value = window.scrollY // 현재 스크롤 위치를 반영\n    }\n\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll)\n    })\n\n    onUnmou