{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, onUnmounted } from 'vue';\nimport { useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const router = useRouter();\n    const menuTop = ref(0);\n    const handleScroll = () => {\n      menuTop.value = window.scrollY;\n    };\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll);\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll);\n    });\n    const navigateTo = path => {\n      router.push(path);\n    };\n    return {\n      navigateTo,\n      menuTop\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","useRouter","setup","router","menuTop","handleScroll","value","window","scrollY","addEventListener","removeEventListener","navigateTo","path","push"],"sources":["D:\\JAVIS\\dongkukDBmon\\dongkukDBmon\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div\n      class=\"left-menu\"\n      :style=\"{ top: `${menuTop}px` }\"\n    >\n      <ul>\n        <li @click=\"navigateTo('/')\">첫화면</li>\n        <li @click=\"navigateTo('/db-list')\">DB 전체 리스트</li>\n        <li @click=\"navigateTo('/sms-history')\">SMS 전송 내역</li>\n        <li @click=\"navigateTo('/threshold-list')\">임계치 리스트</li>\n        <li @click=\"navigateTo('/tablespaces')\">TB 리스트</li>\n      </ul>\n    </div>\n    <div class=\"right-content\">\n      <router-view /> <!-- 라우팅된 컴포넌트를 렌더링 -->\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted, onUnmounted } from 'vue'\nimport { useRouter } from 'vue-router'\n\nexport default {\n  setup() {\n    const router = useRouter()\n    const menuTop = ref(0)\n\n    const handleScroll = () => {\n      menuTop.value = window.scrollY\n    }\n\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll)\n    })\n\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll)\n    })\n\n    const navigateTo = (path) => {\n      router.push(path)\n    }\n\n    return { navigateTo, menuTop }\n  }\n}\n</script>\n\n<style>\n#app {\n  display: flex;\n  height: 100vh;\n}\n\n.left-menu {\n  width: 200px;\n  background: #f4f4f4;\n  padding: 20px;\n  position: fixed; /* 화면의 고정된 위치에 유지 */\n  left: 0; /* 왼쪽에 고정 */\n  top: 0; /* 화면 맨 위로 시작 */\n  bottom: 0; /* 화면 맨 아래까지 확장 */\n  overflow-y: auto; /* 내용이 길면 스크롤 가능 */\n}\n\n.left-menu ul {\n  list-style: none;\n  padding: 0;\n}\n\n.left-menu li {\n  padding: 10px;\n  cursor: pointer;\n}\n\n.left-menu li:hover {\n  background: #ddd;\n}\n\n.right-content {\n  flex: 1;\n  padding: 20px;\n  margin-left: 220px; /* 메뉴의 폭 + 여백 */\n}\n</style>\n"],"mappings":";AAqBA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAU,QAAS,KAAI;AAChD,SAASC,SAAQ,QAAS,YAAW;AAErC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIF,SAAS,CAAC;IACzB,MAAMG,OAAM,GAAIN,GAAG,CAAC,CAAC;IAErB,MAAMO,YAAW,GAAIA,CAAA,KAAM;MACzBD,OAAO,CAACE,KAAI,GAAIC,MAAM,CAACC,OAAM;IAC/B;IAEAT,SAAS,CAAC,MAAM;MACdQ,MAAM,CAACE,gBAAgB,CAAC,QAAQ,EAAEJ,YAAY;IAChD,CAAC;IAEDL,WAAW,CAAC,MAAM;MAChBO,MAAM,CAACG,mBAAmB,CAAC,QAAQ,EAAEL,YAAY;IACnD,CAAC;IAED,MAAMM,UAAS,GAAKC,IAAI,IAAK;MAC3BT,MAAM,CAACU,IAAI,CAACD,IAAI;IAClB;IAEA,OAAO;MAAED,UAAU;MAAEP;IAAQ;EAC/B;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}